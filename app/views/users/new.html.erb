<%= form_for :user do |f| %>

    <%= render 'shared/messages', object: f.object %>

    <p>
        <%= f.label :f_name %><br>
        <%= f.text_field :f_name %>
    </p>

    <p>
        <%= f.label :l_name %><br>
        <%= f.text_field :l_name %>
    </p>

    <p>
        <%= f.label :email %><br>
        <%= f.text_field :email %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
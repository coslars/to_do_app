<% provide(:title, 'All users') %>
<h1>All users</h1>

<div>
    <%= link_to 'Create', new_user_path %>
</div>

<%= will_paginate %>

<ul class="users">
    <% @users.each do |user| %>
        <li>
            <%= link_to "#{user.f_name} #{user.l_name}", action: 'edit', id: user %> - <%= link_to("Delete", "/sessions/:id", method: "delete", id: user) %>
        </li>
    <% end %>
</ul>

<%= will_paginate %>
